    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug STM32G0 (ST-Link)",
            "type": "cortex-debug",
            "request": "launch",
            "servertype": "openocd",
            "cwd": "${workspaceFolder}",
            "executable": "${workspaceFolder}/build/Debug/YLab_STM32G0_Template.elf",
            "device": "STM32G070RB",
            "configFiles": [
                "interface/stlink.cfg",
                "target/stm32g0x.cfg"
            ],
            "runToEntryPoint": "Reset_Handler",
            "showDevDebugOutput": "parsed",
            "armToolchainPath": "/usr/bin",
            "gdbPath": "gdb-multiarch",
            "preLaunchCommands": [
                "monitor halt",
                "monitor reset halt"
            ],
            "postLaunchCommands": [
                "monitor reset run"
            ]
        }
    ]
}